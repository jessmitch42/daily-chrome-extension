{
  "manifest_version": 2,
  "name": "Daily: Video Highlighter",
  "description": "Highlight individual videos in Daily calls",
  "version": "0.1",
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [{
    "css": ["highlight.css"],
    "js": ["highlight.js"],
    "matches": ["https://*.daily.co/*"]
  }],
  "permissions": [
    "https://*.daily.co/*"
  ]
}
